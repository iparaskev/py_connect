//bme680 temperature-humidity-gas sensor.

peripheral:
    name: bme680
    vcc: 3.3
    type: sensor
    operating_voltage: 3.3
    pins:
	- power:
	    name: vcc
	    number: 1
	    type: 5v
	- io_digital: -> sda-0
	    name: sda
	    number: 2
	- io_digital: -> scl-0
	    name: scl
	    number: 3
	- power:
	    name: gnd
	    number: 4
	    type: gnd
