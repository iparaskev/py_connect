//tfmini lidar

peripheral:
    name: tfmini
    vcc: 5
    type: sensor
    operating_voltage: 3.3
    pins:
	- power:
	    name: vcc
	    number: 1
	    type: 5v
	- power:
	    name: gnd
	    number: 2
	    type: gnd
	- io_digital: -> tx-0
	    name: tx
	    number: 3
	- io_digital: -> rx-0
	    name: rx
	    number: 4
