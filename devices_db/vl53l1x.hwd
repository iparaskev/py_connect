//vl53l1x tof sensor

peripheral:
    name: vl53l1x
    vcc: 3.3
    type: sensor
    operating_voltage: 3.3
    pins:
	- power:
	    name: vcc
	    number: 1
	    type: 5v
	- power:
	    name: gnd
	    number: 2
	    type: gnd
	- io_digital: -> sda-0
	    name: sda
	    number: 3
	- io_digital: -> scl-0
	    name: scl
	    number: 4
