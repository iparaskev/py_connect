<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="hw_devices" nsURI="http://www.example.org/hw_devices" nsPrefix="hw_devices">
  <eClassifiers xsi:type="ecore:EClass" name="Device">
    <eStructuralFeatures xsi:type="ecore:EReference" name="pins" upperBound="-1" eType="#//Pin"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operating_voltage" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Pin">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="number" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PowerPin" eSuperTypes="#//Pin"/>
  <eClassifiers xsi:type="ecore:EClass" name="GpioPin" eSuperTypes="#//Pin">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="h_chip" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
        defaultValueLiteral="0"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Power3V3" eSuperTypes="#//PowerPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="conn_to" upperBound="-1"
        eType="#//Power3V3" eOpposite="#//Power3V3/conn_from"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="conn_from" eType="#//Power3V3"
        changeable="false" eOpposite="#//Power3V3/conn_to"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Power5V" eSuperTypes="#//PowerPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="conn_to" upperBound="-1"
        eType="#//Power5V" eOpposite="#//Power5V/conn_from"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="conn_from" eType="#//Power5V"
        changeable="false" eOpposite="#//Power5V/conn_to"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Gnd" eSuperTypes="#//PowerPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="conn_to" upperBound="-1"
        eType="#//Gnd" eOpposite="#//Gnd/conn_from"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="conn_from" upperBound="-1"
        eType="#//Gnd" changeable="false" eOpposite="#//Gnd/conn_to"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Input" eSuperTypes="#//GpioPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="conn_from" eType="#//Output"
        eOpposite="#//Output/conn_to"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Output" eSuperTypes="#//GpioPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="conn_to" eType="#//Input"
        eOpposite="#//Input/conn_from"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="I2cSda" eSuperTypes="#//GpioPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="1008"
        eType="#//I2cSda"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="I2cScl" eSuperTypes="#//GpioPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="1008"
        eType="#//I2cScl"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Pwm" eSuperTypes="#//GpioPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connection" eType="#//Pwm"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="UartRx" eSuperTypes="#//GpioPin"/>
  <eClassifiers xsi:type="ecore:EClass" name="UartTx" eSuperTypes="#//GpioPin"/>
  <eClassifiers xsi:type="ecore:EClass" name="SpiMiso" eSuperTypes="#//GpioPin"/>
  <eClassifiers xsi:type="ecore:EClass" name="SpiSclk" eSuperTypes="#//GpioPin"/>
  <eClassifiers xsi:type="ecore:EClass" name="SpiMosi" eSuperTypes="#//GpioPin"/>
  <eClassifiers xsi:type="ecore:EClass" name="SpiCe" eSuperTypes="#//GpioPin"/>
</ecore:EPackage>
