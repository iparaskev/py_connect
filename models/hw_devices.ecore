<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="hw_devices" nsURI="http://www.example.org/hw_devices" nsPrefix="hw_devices">
  <eClassifiers xsi:type="ecore:EClass" name="Device">
    <eStructuralFeatures xsi:type="ecore:EReference" name="pins" upperBound="-1" eType="#//Pin"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operating_voltage" upperBound="5"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Pin">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="number" upperBound="400"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PowerPin" eSuperTypes="#//Pin"/>
  <eClassifiers xsi:type="ecore:EClass" name="GpioPin" eSuperTypes="#//Pin"/>
  <eClassifiers xsi:type="ecore:EClass" name="I2cPin" eSuperTypes="#//Pin"/>
  <eClassifiers xsi:type="ecore:EClass" name="SpiPin"/>
  <eClassifiers xsi:type="ecore:EClass" name="UartPin"/>
  <eClassifiers xsi:type="ecore:EClass" name="PwmPin"/>
  <eClassifiers xsi:type="ecore:EClass" name="Power3V3" eSuperTypes="#//PowerPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="-1"
        eType="#//Power3V3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Power5V" eSuperTypes="#//PowerPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="-1"
        eType="#//Power5V"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Gnd" eSuperTypes="#//PowerPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="-1"
        eType="#//Gnd"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Input" eSuperTypes="#//GpioPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connection" eType="#//Input"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Output" eSuperTypes="#//GpioPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connection" eType="#//Output"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Sda" eSuperTypes="#//I2cPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="1008"
        eType="#//Sda"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Scl" eSuperTypes="#//I2cPin">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="1008"
        eType="#//Scl"/>
  </eClassifiers>
</ecore:EPackage>
